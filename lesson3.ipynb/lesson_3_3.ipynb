   ],
   "source": [
    "import math\n",
    "side = float(input(\"請輸入對邊:\"))\n",
    "another_side = float(input(\"請輸入斜邊:\"))\n",
    "radians = math.asin(side / another_side)\n",
    "degree = math.degrees(radians)\n",
    "print(f\"對邊:{side} 斜邊:{another_side} 徑度是{radians} 角度:{round(degree,ndigits=2)}\")\n"
   ]
  }
